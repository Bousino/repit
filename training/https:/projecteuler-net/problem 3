
package com.company;

public class Main {
/*
The prime factors of 13195 are 5, 7, 13 and 29.
What is the largest prime factor of the number 600851475143 ?
 */

    public static void main(String[] args) {

        long a = 600851475143L;
        int i;
        for (i = 2; i <= a; i++) {
            if (a % i == 0) {
                a /= i;
                i--;
            }
        }
        System.out.println(i);
    }
}
